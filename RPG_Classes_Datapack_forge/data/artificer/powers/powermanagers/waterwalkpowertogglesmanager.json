{
  "type":"origins:multiple",

  "waterwalkswitchmanager": {

    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:if_else_list",
        "actions": [
        {
          "condition": {
            "type": "origins:and",
            "conditions": [
              {
                "type":"origins:resource",
                "resource":"artificer:powertoggles_master",
                "comparison":"==",
                "compare_to":1

              },
              {
                "type": "origins:or",
                "conditions": [
                  {
                    "type":"origins:resource",
                    "resource":"artificer:equipmenttoggles_ironboots",
                    "comparison":"==",
                    "compare_to":1
                  },
                  {
                    "type":"origins:resource",
                    "resource":"artificer:equipmenttoggles_diamondboots",
                    "comparison":"==",
                    "compare_to":1
                  },
                  {
                    "type":"origins:resource",
                    "resource":"artificer:equipmenttoggles_netheriteboots",
                    "comparison":"==",
                    "compare_to":1
                  }
                ]
              },
              {
                "type": "origins:power_active",
                "power": "artificer:crafteditempowers/waterwalkswitch",
                "inverted": true
              }
            ]

          },
          "action": {
                "type": "origins:grant_power",
                "power": "artificer:powerswitches/waterwalkswitch",
                "source": "artificer:artificer"
          }
        },
        {
          "condition": {
            "type": "origins:or",
            "conditions": [
              {
                "type":"origins:resource",
                "resource":"artificer:powertoggles_master",
                "comparison":"==",
                "compare_to":0

              },
              {
                "type": "origins:and",
                "conditions": [
                  {
                    "type":"origins:resource",
                    "resource":"artificer:equipmenttoggles_ironboots",
                    "comparison":"==",
                    "compare_to":0
                  },
                  {
                    "type":"origins:resource",
                    "resource":"artificer:equipmenttoggles_diamondboots",
                    "comparison":"==",
                    "compare_to":0
                  },
                  {
                    "type":"origins:resource",
                    "resource":"artificer:equipmenttoggles_netheriteboots",
                    "comparison":"==",
                    "compare_to":0
                  }
                ]
              }
            ]
          },
          "action":{

                "type": "origins:revoke_power",
                "power": "artificer:powerswitches/waterwalkswitch",
                "source": "artificer:artificer"
          }
        }
      ]
    }
  }
}