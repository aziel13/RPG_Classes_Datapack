{
   "type":"origins:multiple",


   "slowessencecheck": {

      "type": "origins:action_over_time",
      "rising_action": {
         "type": "origins:and",
         "actions": [
            {
               "type":"origins:change_resource",
               "resource":"artificer:essencetoggles_slowcollect",
               "operation": "set",
               "change":1
            },
            {
               "type": "origins:execute_command",
               "command": "say entered range of low mana density source"
            }
         ]
      },
      "entity_action": {
         "type": "origins:if_else",
         "condition": {
            "type":"origins:resource",
            "resource":"artificer:essencetoggles_slowcollect",
            "comparison":"==",
            "compare_to":0
         },
         "if_action": {
            "type": "origins:and",
            "actions": [
               {
                  "type": "origins:execute_command",
                  "command": "resource set @p artificer:essencetoggles_slowcollect 1"

               },
               {
                  "type": "origins:execute_command",
                  "command": "say entered range of low mana density source"
               }
            ]
         }
      },
      "falling_action": {
         "type": "origins:and",
         "actions": [
            {
               "type":"origins:change_resource",
               "resource":"artificer:essencetoggles_slowcollect",
               "operation": "set",
               "change":0
            },
            {
               "type": "origins:execute_command",
               "command": "say left range of low mana density source"
            }
         ]
      },
      "interval": 20,
      "condition": {

         "type": "origins:block_in_radius",
         "block_condition": {
            "type": "origins:or",
            "conditions": [
               {
                  "type": "origins:material",
                  "material": "solid_organic"
               },
               {
