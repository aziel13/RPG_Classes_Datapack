{
  "type":"origins:multiple",

  "diamondhelmetequipcheck": {

    "type": "origins:action_over_time",
    "rising_action": {
      "type": "origins:and",
      "actions": [
        {
          "type":"origins:change_resource",
          "resource":"artificer:resources/equipmenttoggles_diamondhelmet",
          "operation": "set",
          "change":1
        },
        {
          "type": "origins:execute_command",
          "command": "say Diamond Helm Of True Sight Equiped"
        }
      ]
    },
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type":"origins:resource",
        "resource":"artificer:resources/equipmenttoggles_diamondhelmet",
        "comparison":"==",
        "compare_to":0
      },
      "if_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command": "resource set @p artificer:resources/equipmenttoggles_diamondhelmet 1"

          },
          {
            "type": "origins:execute_command",
            "command": "say Netherite Omnithopter Chestplate Equiped"
          }
        ]
      }
    },
    "falling_action": {
      "type": "origins:and",
      "actions": [
        {
          "type":"origins:change_resource",
          "resource":"artificer:resources/equipmenttoggles_diamondhelmet",
          "operation": "set",
          "change":0
        },
        {
          "type": "origins:execute_command",
          "command": "say Diamond Helm Of True Sight Unequiped"
        }
      ]
    },
    "interval": 20,
    "condition": {
      "type": "origins:equipped_item",
      "equipment_slot": "head",
      "item_condition": {
        "type": "origins:and",
        "conditions": [
          {
            "type": "origins:ingredient",
            "ingredient": {
              "item": "minecraft:diamond_helmet"
            }
          },
          {
            "type": "origins:nbt",
            "nbt": "{CustomModelData: 1952314}"
          }
        ]
      }
    }
  },
