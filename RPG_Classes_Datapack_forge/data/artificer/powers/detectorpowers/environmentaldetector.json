{
  "type":"origins:multiple",

  "hotenvironmentcheck": {

    "type": "origins:action_over_time",
    "rising_action": {
      "type": "origins:and",
      "actions": [
        {
          "type":"origins:change_resource",
          "resource":"artificer:enviornmentaltoggles_hotenvironment",
          "operation": "set",
          "change":1
        },
        {
          "type": "origins:execute_command",
          "command": "say Entering Hot Environment"
        }
      ]
    },
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type":"origins:resource",
        "resource":"artificer:enviornmentaltoggles_hotenvironment",
        "comparison":"==",
        "compare_to":0
      },
      "if_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command": "resource set @p artificer:enviornmentaltoggles_hotenvironment 1"

          },
          {
            "type": "origins:execute_command",
            "command": "say Entering Hot Environment"
          }
        ]
      }
    },
    "falling_action": {
      "type": "origins:and",
      "actions": [
        {
          "type":"origins:change_resource",
          "resource":"artificer:enviornmentaltoggles_hotenvironment",
          "operation": "set",
          "change":0
        },
        {
          "type": "origins:execute_command",
          "command": "say Leaving Hot Environment"
        }
      ]
    },
    "interval": 20,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:biome",
          "condition": {
            "type": "origins:temperature",
            "comparison": ">=",
            "compare_to": 2
          }
        }
      ]
    }
  },

