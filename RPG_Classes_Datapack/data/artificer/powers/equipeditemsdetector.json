{
  "type":"origins:multiple",

  "fungusonstickmainhandcheck": {

    "type": "origins:action_over_time",
    "rising_action": {
      "type": "origins:and",
      "actions": [
        {
          "type":"origins:change_resource",
          "resource":"artificer:equipmenttoggles_fungusonstickmainhand",
          "operation": "set",
          "change":1
        },
        {
          "type": "origins:execute_command",
          "command": "say Artificer's tool Equiped in Mainhand"
        }
      ]
    },
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type":"origins:resource",
        "resource":"artificer:equipmenttoggles_fungusonstickmainhand",
        "comparison":"==",
        "compare_to":0
      },
      "if_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command": "resource set @p artificer:equipmenttoggles_fungusonstickmainhand 1"

          },
          {
            "type": "origins:execute_command",
            "command": "say Artificer's tool Equiped in Mainhand"
          }
        ]
      }
    },
    "falling_action": {
      "type": "origins:and",
      "actions": [
        {
          "type":"origins:change_resource",
          "resource":"artificer:equipmenttoggles_fungusonstickmainhand",
          "operation": "set",
          "change":0
        },
        {
          "type": "origins:execute_command",
          "command": "say Artificer's tool Unequiped in Mainhand"
        }
      ]
    },
    "interval": 20,
    "condition": {
      "type": "origins:equipped_item",
      "equipment_slot": "mainhand",
      "item_condition": {
        "type": "origins:and",
        "conditions": [
          {
            "type":"origins:ingredient",
            "ingredient":{
              "item":"minecraft:warped_fungus_on_a_stick"
            }

          },
          {
            "type": "origins:nbt",
            "nbt": "{CustomModelData: 1}"
          }
        ]
      }
    }
  },
